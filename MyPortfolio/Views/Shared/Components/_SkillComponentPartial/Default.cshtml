@model List<MyPortfolio.DAL.Entities.Skill>

<div class="col-six tab-full right">
    <h3>Programlama Dilleri & Teknolojiler</h3>

    <ul class="skill-bars">

        @foreach (var item in Model)
        {
            string per = "progress percent";
            per += item.Value.ToString();
            <li>
                <div class="@per"><span>@item.Value%</span></div>
                <strong>@item.Title</strong>
            </li>
        }
    </ul>
</div>